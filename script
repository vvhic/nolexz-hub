                                    if Aiming then
                                        Aiming:Remove()
                                    end
                                    local Aim = Instance.new("Part", Workspace)
                                    Aim.Anchored = true
                                    Aim.CanCollide = false
                                    Aim.Transparency = 0.5
                                    Aim.Size = Vector3.new(10, 10, 10)
                                    Aim.Name = "Aim"
                                    while task.wait() do
                                        Aim.CFrame = CFrame.new(-16.9939995, 41.5, -45.9039993)
                                        task.wait()
                                        Aim.CFrame = CFrame.new(-16.9939995, 43, -45.9039993)
                                        task.wait()
                                        Aim.CFrame = CFrame.new(-16.9939995, 42.5, -45.9039993)
                                        task.wait()
                                        Aim.CFrame = CFrame.new(-16.9939995, 44.5, -45.9039993)
                                        task.wait()
                                        Aim.CFrame = CFrame.new(-16.9939995, 44, -45.9039993)
                                        task.wait()
                                        Aim.CFrame = CFrame.new(-16.9939995, 40, -45.9039993)
                                    end
                                end
                                if Key == Prefix.Aim2 then
                                    if Workspace:FindFirstChild("Aim") then
                                        Workspace:FindFirstChild("Aim"):Remove()
                                    end
                                    if Aiming then
                                        Aiming:Remove()
                                    end
                                    local Aim2 = Instance.new("Part", Workspace)
                                    Aim2.Anchored = true
                                    Aim2.CanCollide = false
                                    Aim2.Transparency = 0.5
                                    Aim2.Size = Vector3.new(10, 10, 10)
                                    Aim2.Name = "Aim2"
                                    while task.wait() do
                                        Aim2.CFrame = CFrame.new(-16.8939991, 41.5, 142.925003)
                                        task.wait()
                                        Aim2.CFrame = CFrame.new(-16.8939991, 43, 142.925003)
                                        task.wait()
                                        Aim2.CFrame = CFrame.new(-16.8939991, 42.5, 142.925003)
                                        task.wait()
                                        Aim2.CFrame = CFrame.new(-16.8939991, 44.5, 142.925003)
                                        task.wait()
                                        Aim2.CFrame = CFrame.new(-16.8939991, 44, 142.925003)
                                        task.wait()
                                        Aim2.CFrame = CFrame.new(-16.8939991, 40, 142.925003)
                                    end
                                end
                                if Key == Prefix.Default then
                                    for _, v in next, Workspace:GetDescendants() do
                                        if v:IsA("Part") and v.Name == "Aim" or v.Name == "Aim2" then
                                            v:Remove()
                                        end
                                    end
                                end
                            end
                        )
                    end
                )

                local MouseControls = UI.Frame.Frame.AB:Clone()
                MouseControls.Name = "MC"
                MouseControls.Parent = UI.Frame.Frame
                game:GetService("CoreGui").ScreenGui.Frame.Frame.MC.Label.Text = "MouseControl"
                MouseControls.Button.MouseButton1Click:Connect(
                    function()
                        game:GetService("StarterGui"):SetCore(
                            "SendNotification",
                            {
                                Title = "Keybinds",
                                Text = "MouseControl = e, StopMouseControl = u",
                                Duration = 10,
                                Button1 = "OK!"
                            }
                        )
                        local Player = game:GetService("Players").LocalPlayer
                        local Workspace = game:GetService("Workspace")
                        local Run

                        local Mouse = Player:GetMouse()
                        local Prefix = {
                            MouseControl = "e",
                            Stop = "u"
                        }

                        Mouse.KeyDown:Connect(
                            function(Key)
                                if Key == Prefix.MouseControl then
                                    local Folder = Player.Character:FindFirstChildOfClass("Folder")
                                    if Folder and Wait() then
                                        local BodyPosition = Instance.new("BodyPosition", Folder.Handle)
                                        BodyPosition.D = 250
                                        BodyPosition.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
                                        Folder.Handle:FindFirstChildOfClass("Weld"):Remove()
                                        Run =
                                            game:GetService("RunService").Heartbeat:Connect(
                                            function()
                                                Folder.Handle.CFrame =
                                                    (CFrame.new(
                                                    Mouse.Hit.Position.X,
                                                    Mouse.Hit.Position.Y,
                                                    Mouse.Hit.Position.Z
                                                )) * CFrame.Angles(0, 0, 0)
                                                BodyPosition.Position =
                                                    Vector3.new(
                                                    Mouse.Hit.Position.X,
                                                    Mouse.Hit.Position.Y,
                                                    Mouse.Hit.Position.Z
                                                )
                                                Folder.Handle.Velocity = Vector3.new(30, 0, 0)
                                                for _, v in next, Player.Character:GetDescendants() do
                                                    if v:IsA("BasePart") and v.Name ~= "HumanoidRootPart" then
                                                        v.Velocity = Vector3.new(30, 0, 0)
                                                    end
                                                end
                                            end
                                        )
                                    end
                                end
                                if Key == Prefix.Stop then
                                    if Run then
                                        Run:Disconnect()
                                    end
                                    Folder.Handle:FindFirstChildOfClass("BodyPosition"):Remove()
                                end
                            end
                        )
                    end
                )

                local SilentAimbot = UI.Frame.Frame.AB:Clone()
                SilentAimbot.Name = "SA"
                SilentAimbot.Parent = UI.Frame.Frame
                game:GetService("CoreGui").ScreenGui.Frame.Frame.SA.Label.Text = "SilentAimbot"
                MouseControls.Button.MouseButton1Click:Connect(
                    function()
                        local Player = game.Players.LocalPlayer
                        local Camera = workspace.CurrentCamera
                        Holding = false
                        e = false
                        isdoing = false
                        function findGoals()
                            for i, v in pairs(workspace:GetDescendants()) do
                                if v.Name == "Goal" then
                                    v.Parent = workspace
                                end
                            end
                        end
                        local function findClosestPart(position)
                            local closestPart, closestPartMagnitude
                            local tmpMagnitude
                            for i, v in pairs(workspace:GetChildren()) do
                                if v.Name == "Goal" then
                                    if closestPart then
                                        tmpMagnitude = (position - v.Position).magnitude
                                        if tmpMagnitude < closestPartMagnitude then
                                            closestPart = v
                                            closestPartMagnitude = tmpMagnitude
                                        end
                                    else
                                        closestPart = v
                                        closestPartMagnitude = (position - v.Position).magnitude
                                    end
                                end
                            end
                            return closestPart
                        end
                        --//vb
                        local player = game.Players.LocalPlayer
                        local camera = game.Workspace.CurrentCamera
                        local character = player.Character
                        local head = character.Head
                        local torso = character.Torso
                        findGoals()
                        local function cam()
                            while wait() do
                                if isdoing == true then
                                    local pos = head.Position
                                    local partyGoal = findClosestPart(head.Position)
                                    local lookAt = partyGoal.Position + Vector3.new(0, 90, 0)
                                    local mag = (character.PrimaryPart.Position - partyGoal.Position).magnitude
                                    local cameraCFrame = CFrame.new(pos, lookAt)
                                    Camera.CFrame = cameraCFrame
                                else
                                    return nil
                                end
                            end
                        end

                        game.Players.LocalPlayer.Character.ChildAdded:Connect(
                            function(child)
                                local pv = game.Players.LocalPlayer.Character.Basketball
                                local UIS = game:GetService("UserInputService")

                                game.Players.LocalPlayer.Character:WaitForChild("Humanoid").Jumping:Connect(
                                    function()
                                        isdoing = true
                                        if game.Players.LocalPlayer.Character.Basketball then
                                            local lookthat =
                                                findClosestPart(game.Players.LocalPlayer.Character.Torso.Position)
                                            local pvm =
                                                (lookthat.Position - game.Players.LocalPlayer.Character.Torso.Position).magnitude
                                            if pvm >= 30 then
                                                if pvm <= 44 then
                                                    pv.PowerValue.Value = 75
                                                    cam()
                                                end
                                            end
                                            local lookthat =
                                                findClosestPart(game.Players.LocalPlayer.Character.Torso.Position)
                                            local pvm =
                                                (lookthat.Position - game.Players.LocalPlayer.Character.Torso.Position).magnitude
                                            if pvm >= 45 then
                                                if pvm <= 53 then
                                                    pv.PowerValue.Value = 80
                                                    cam()
                                                end
                                            end
                                            local lookthat =
                                                findClosestPart(game.Players.LocalPlayer.Character.Torso.Position)
                                            local pvm =
                                                (lookthat.Position - game.Players.LocalPlayer.Character.Torso.Position).magnitude
                                            if pvm >= 54 then
                                                if pvm <= 66 then
                                                    pv.PowerValue.Value = 85
                                                    cam()
                                                end
                                            end
                                            local lookthat =
                                                findClosestPart(game.Players.LocalPlayer.Character.Torso.Position)
                                            local pvm =
                                                (lookthat.Position - game.Players.LocalPlayer.Character.Torso.Position).magnitude
                                            if pvm >= 67 then
                                                if pvm <= 76 then
                                                    pv.PowerValue.Value = 90
                                                    cam()
                                                end
                                            end
                                            local lookthat =
                                                findClosestPart(game.Players.LocalPlayer.Character.Torso.Position)
                                            local pvm =
                                                (lookthat.Position - game.Players.LocalPlayer.Character.Torso.Position).magnitude
                                            if pvm >= 77 then
                                                if pvm <= 84 then
                                                    pv.PowerValue.Value = 95
                                                    cam()
                                                end
                                            end
                                            local lookthat =
                                                findClosestPart(game.Players.LocalPlayer.Character.Torso.Position)
                                            local pvm =
                                                (lookthat.Position - game.Players.LocalPlayer.Character.Torso.Position).magnitude
                                            if pvm >= 85 then
                                                if pvm <= 91 then
                                                    pv.PowerValue.Value = 100
                                                    cam()
                                                end
                                            end
                                        end
                                    end
                                )
                            end
                        )

                        local UIS = game:service("UserInputService")
                        UIS.InputBegan:connect(
                            function(l)
                                if l.UserInputType == Enum.UserInputType.MouseButton1 then
                                    isdoing = false
                                end
                            end
                        )
                    end
                )
            end
        )
    end
)
