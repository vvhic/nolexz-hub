        local MouseControls = UI.Frame.Frame.AB:Clone()
        MouseControls.Name = "MC"
        MouseControls.Parent = UI.Frame.Frame
        game:GetService("CoreGui").ScreenGui.Frame.Frame.MC.Label.Text = "MouseControl"
